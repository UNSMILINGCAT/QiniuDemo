<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ycwh.demo.chapter4.DinosaurMapper">
    <resultMap id="result_map" type="dinosaur">
        <result property="id" column="id"/>
        <result property="dinosaurName" column="name"/>
        <result property="sex" column="sex" typeHandler="com.ycwh.demo.chapter4.SexEnumTypeHandler"/>
        <result property="lv" column="lv"/>
    </resultMap>
    <select id="selectAll" resultMap="result_map" databaseId="mysql">
        select * from ssm.dinosaur
    </select>
    <insert id="insert" parameterType="dinosaur">
        insert into ssm.dinosaur(name, sex, lv) values (#{dinosaurName},#{sex,typeHandler=com.ycwh.test.demo.chapter4.SexEnumTypeHandler},#{lv})
    </insert>
</mapper>